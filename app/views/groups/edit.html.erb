<div>
  <h1>グループ編集画面</h1>
</div>
<div>
  <div>
    <div>
      <div>メンバー</div>
      <% @group_users.each do |u|%>
        <div><%= u.name%></div>
      <%end%>
    </div>
    <%= form_with model: @group do |f| %>
      <%= f.collection_select :user_id, @other_users, :id, :name, include_blank: "ユーザーの選択" %>
      <%= f.submit "追加"%>
    <% end %>
  </div>
</div>